{
  "private": false,
  "name": "react-vite-admin",
  "version": "0.1.0",
  "homepage": "https://yourenz.github.io/react-vite-admin",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "type-check": "tsc",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "lint:fix": "eslint ./src --ext .ts,.tsx,js --quiet --fix --ignore-path ./.eslintignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{ts,tsx,js}\"",
    "lint": "yarn lint:format && yarn lint:fix"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": "yarn lint"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.1",
    "antd": "^4.15.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^6.0.0",
    "less": "^4.1.1",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.11",
    "typescript": "^4.1.2",
    "vite": "^2.1.5",
    "vite-plugin-compression": "^0.2.4",
    "vite-plugin-html": "^2.0.7",
    "vite-plugin-style-import": "^0.10.0",
    "vite-plugin-svg-icons": "^0.5.0"
  },
  "license": "MIT"
}
