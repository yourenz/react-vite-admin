{
  "private": false,
  "name": "react-vite-admin",
  "version": "0.1.0",
  "homepage": "https://yourenz.github.io/react-vite-admin",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "type-check": "tsc",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "lint:fix": "eslint --fix",
    "lint:format": "prettier --write .",
    "lint": "yarn lint:format && yarn lint:fix"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": "yarn lint:fix",
    "*.{ts,tsx,js,html,css,scss,less,md,json}": "yarn lint:format"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.0",
    "antd": "^4.16.6",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "@vitejs/plugin-react-refresh": "^1.3.4",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^6.0.0",
    "less": "^4.1.1",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "sass": "^1.35.1",
    "typescript": "^4.3.4",
    "vite": "^2.3.8",
    "vite-plugin-compression": "^0.2.5",
    "vite-plugin-html": "^2.0.7",
    "vite-plugin-style-import": "^1.0.1",
    "vite-plugin-svg-icons": "^1.0.0"
  },
  "license": "MIT"
}
